<figTreeAnnotationExporter>

	<figTreeAnnotation>
		<annotationName>sequenceID</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.sequenceID')}</valueFreemarkerTemplate>
	</figTreeAnnotation>

	<figTreeAnnotation>
		<annotationName>genotype</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.lineage')}</valueFreemarkerTemplate>
	</figTreeAnnotation>

	<figTreeAnnotation>
		<annotationName>clade</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.clade')}</valueFreemarkerTemplate>
	</figTreeAnnotation>

	<figTreeAnnotation>
		<annotationName>combined</annotationName>
		<valueFreemarkerTemplate>${renderNestedProperty('sequence.lineage')}/${renderNestedProperty('sequence.clade')}</valueFreemarkerTemplate>
	</figTreeAnnotation>


</figTreeAnnotationExporter>
