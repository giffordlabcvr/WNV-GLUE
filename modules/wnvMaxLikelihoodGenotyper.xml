<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>wnvMaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>

	<cladeCategory>
		<name>lineage</name>	
		<displayName>Lineage</displayName>	
		<whereClause>clade_category = 'lineage'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
	    <distanceCutoff>0.1</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>		
	</cladeCategory>

	<cladeCategory>
		<name>clade</name>	
		<displayName>Clade</displayName>	
		<whereClause>clade_category = 'clade'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
	    <distanceCutoff>0.05</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>		
	</cladeCategory>

</maxLikelihoodGenotyper>



